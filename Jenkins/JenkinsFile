properties([ 
	parameters([
	booleanParam(name: 'Jenkins', description:'Keep this checked if you want to run this project Jenkins file', default:true)
	])
	    
	   

 ])

pipeline{ 

    agent any    
        stages{           
            stage('FrameworkSample'){                  
                    steps{
                        echo 'Hello world'
                    }
                }
            }            
        
        post{
            always{
                echo 'Say Hello again'
            }
        }
}
