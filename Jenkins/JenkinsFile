properties([ 
	parameters([
	string(name: 'Jenkins',description:'Keep this checked if you want to run this project Jenkins file' )
	
	])
 ])

pipeline{ 

    agent any    
        stages{           
            stage('FrameworkSample'){                  
                    steps{
                        echo 'Hello world'
                        echo "Hello ${params.Jenkins}" 
                    }
                }

            }            
        
        post{
            always{
                echo 'Say Hello again'
            }
        }
}
